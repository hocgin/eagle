<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="in.hocg.eagle.modules.wx.mapper.WxMenuMapper">

  <select id="paging" resultType="in.hocg.eagle.modules.wx.entity.WxMenu">
    SELECT wm.*
    FROM wx_menu wm
    ORDER BY wm.created_at DESC
  </select>
</mapper>
