<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="in.hocg.eagle.modules.com.mapper.RequestLogMapper">

  <select id="pagingWithComplex" resultType="in.hocg.eagle.modules.com.entity.RequestLog">
    SELECT crl.*
    FROM com_request_log crl
    ORDER BY crl.created_at DESC
  </select>
</mapper>
