<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="in.hocg.eagle.modules.mkt.mapper.CouponProductCategoryRelationMapper">

    <select id="selectAllProductCategoryByCouponId"
            resultType="in.hocg.eagle.modules.mkt.entity.CouponProductCategoryRelation">
      SELECT mcpcr.* FROM mkt_coupon_product_category_relation mcpcr
      WHERE mcpcr.coupon_id = #{couponId}
    </select>
</mapper>
