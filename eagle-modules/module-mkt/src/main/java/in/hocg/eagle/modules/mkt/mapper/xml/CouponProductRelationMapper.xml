<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="in.hocg.eagle.modules.mkt.mapper.CouponProductRelationMapper">

    <select id="selectAllProductByCouponId"
            resultType="in.hocg.eagle.modules.mkt.entity.CouponProductRelation">
      SELECT mcpr.* FROM mkt_coupon_product_relation mcpr
      WHERE mcpr.coupon_id = #{couponId}
    </select>
</mapper>
