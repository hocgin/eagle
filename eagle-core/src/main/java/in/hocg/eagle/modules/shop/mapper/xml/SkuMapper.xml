<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="in.hocg.eagle.modules.shop.mapper.SkuMapper">

    <delete id="deleteAllByProductId">
        DELETE ss FROM shop_sku ss
        WHERE ss.product_id = #{productId}
    </delete>
    <select id="selectListByProductId2" resultType="in.hocg.eagle.modules.shop.entity.Sku">
        SELECT ss.* FROM shop_sku ss
        WHERE ss.product_id = #{productId}
    </select>
</mapper>
